---
layout: default
---
<div id="personal">
{{ content }}
</div>

<div id="related">
  <h2>Related Personals</h2>
  <ul class="personals">
    {% for personal in site.related_personals limit:3 %}
      <li><span>{{ personal.date | date_to_string }}</span> &raquo; <a href="{{ personal.url }}">{{ personal.title }}</a></li>
    {% endfor %}
  </ul>
</div>